{
  "log": {
    "level": "{{ sing_box_log_level }}"
  },
  "inbounds": [
    {
      "type": "tun",
      "interface_name": "{{ sing_box_inbound.interface_name }}",
      "domain_strategy": "{{ sing_box_inbound.domain_strategy }}",
      "inet4_address": "{{ sing_box_inbound.inet4_address }}",
      "auto_route": {{ sing_box_inbound.auto_route | lower }},
      "strict_route": {{ sing_box_inbound.strict_route | lower }},
      "sniff": {{ sing_box_inbound.sniff | lower }}
    }
  ],
  "outbounds": [
    {
      "type": "{{ sing_box_outbound.type }}",
      "server": "{{ sing_box_outbound.server }}",
      "server_port": {{ sing_box_outbound.server_port }},
      "uuid": "{{ sing_box_outbound.uuid }}",
      "flow": "{{ sing_box_outbound.flow }}",
      "tls": {
        "enabled": {{ sing_box_outbound.tls.enabled | lower }},
        "insecure": {{ sing_box_outbound.tls.insecure | lower }},
        "server_name": "{{ sing_box_outbound.tls.server_name }}",
        "utls": {
          "enabled": {{ sing_box_outbound.utls.enabled | lower }},
          "fingerprint": "{{ sing_box_outbound.utls.fingerprint }}"
        },
        "reality": {
          "enabled": {{ sing_box_outbound.reality.enabled | lower }},
          "public_key": "{{ sing_box_outbound.reality.public_key }}",
          "short_id": "{{ sing_box_outbound.reality.short_id }}"
        }
      }
    }
  ],
  "route": {
    "auto_detect_interface": {{ sing_box_route.auto_detect_interface | lower }}
  }
}
